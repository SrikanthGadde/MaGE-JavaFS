package com.telstra.concurrency;

public class Main {

	public static void main(String[] args) {
		
		MathOperation m  = new MathOperation();
		
		AddThread t1 = new AddThread(m, " 1");
		AddThread t2 = new AddThread(m, " 2");
		AddThread t3 = new AddThread(m, " 3");
		
		SubThread t4 = new SubThread(m, " 4");
		SubThread t5 = new SubThread(m, " 5");
		SubThread t6 = new SubThread(m, " 6");
		SubThread t6 = new SubThread(m, " 6");
		SubThread t6 = new SubThread(m, " 6");

	}

}
